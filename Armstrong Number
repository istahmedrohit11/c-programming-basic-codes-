#include<stdio.h>
int main(){
    int n;
    printf("Enter any number : ");
    scanf("%d",&n);
    int sum=0,r;
     int num=n;
    while(n>0){
        r=n%10;
        sum=sum+(r*r*r);
        n=n/10;
    }
    if(num==sum){
        printf("Armstrong Number");
    }
    else{
         printf("Not Armstrong Number");
    }
    return 0;
}
